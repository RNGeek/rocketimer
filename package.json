{
  "name": "rocketimer",
  "version": "1.0.0",
  "description": "Rocketimer is a cowntdown timer, which is simple and high functional.",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "private": true,
  "repository": "RNGeek/rocketimer",
  "scripts": {
    "start": "yarn run dev",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development",
    "check": "run-s check:*",
    "check:tsc": "run-s check:tsc:*",
    "check:tsc:frontend": "tsc -p tsconfig.frontend.json --noEmit",
    "check:tsc:test": "tsc -p tsconfig.test.json --noEmit",
    "check:tsc:benchmark": "tsc -p tsconfig.benchmark.json --noEmit",
    "check:eslint": "eslint '{src,test,benchmark}/**/*.{ts,tsx}'",
    "check:prettier": "prettier --check .",
    "test": "jest -c jest.test.js",
    "coverage": "yarn run test --coverage",
    "benchmark": "run-s benchmark:*",
    "benchmark:fps": "babel-node --extensions '.ts' benchmark/fps.ts",
    "benchmark:render-time": "node --expose-gc $(npm bin)/jest -c jest.benchmark.js --runTestsByPath benchmark/render-time.tsx"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@mizdra/strictly-typed-event-target": "^2.0.0",
    "@react-hook/size": "^2.1.1",
    "konva": "^7.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.9.1",
    "recoil": "^0.0.13",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@mizdra/eslint-config-mizdra": "^0.5.0",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.22",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "canvas": "^2.6.1",
    "copy-webpack-plugin": "^6.2.0",
    "core-js": "^3.6.5",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "jest": "^26.5.2",
    "jest-date-mock": "^1.0.8",
    "jest-performance-testing": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.4.2",
    "prettier": "^2.1.2",
    "react-performance-testing": "^1.2.3",
    "sw-precache-webpack-plugin": "^1.0.0",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^8.0.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.14.8"
  }
}
