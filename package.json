{
  "name": "rocketimer",
  "version": "1.0.0",
  "description": "Rocketimer is a cowntdown timer, which is simple and high functional.",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "private": true,
  "repository": "RNGeek/rocketimer",
  "scripts": {
    "start": "yarn run dev",
    "build": "webpack --mode production",
    "dev": "webpack-cli serve --mode development",
    "check": "run-s -c check:*",
    "check:tsc": "run-s -c check:tsc:*",
    "check:tsc:frontend": "tsc -p tsconfig.frontend.json --noEmit",
    "check:tsc:test": "tsc -p tsconfig.test.json --noEmit",
    "check:tsc:benchmark": "tsc -p tsconfig.benchmark.json --noEmit",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "test": "jest -c jest.test.js",
    "coverage": "yarn run test --coverage",
    "benchmark": "run-s -c benchmark:*",
    "benchmark:fps": "babel-node --extensions '.ts' benchmark/fps.ts",
    "benchmark:memory": "babel-node --extensions '.ts' benchmark/memory.ts",
    "benchmark:render-time": "node --expose-gc $(npm bin)/jest -c jest.benchmark.js --runTestsByPath benchmark/render-time.tsx"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@mizdra/strictly-typed-event-target": "^2.0.0",
    "@react-hook/size": "^2.1.1",
    "@ungap/event-target": "^0.2.2",
    "konva": "^7.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.5",
    "recoil": "^0.1.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@mizdra/eslint-config-mizdra": "^0.8.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.2.2",
    "canvas": "^2.7.0",
    "copy-webpack-plugin": "^6.4.1",
    "core-js": "^3.8.3",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-nibble": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "http-server": "^0.12.3",
    "jest": "^26.5.2",
    "jest-date-mock": "^1.0.8",
    "jest-performance-testing": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.4.2",
    "prettier": "^2.1.2",
    "react-performance-testing": "^1.2.3",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.7",
    "webpack": "^5.3.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^6.0.0-alpha.3"
  },
  "resolutions": {
    "*/**/@babel/runtime": "7.13.10"
  },
  "engines": {
    "node": ">= v14.16.0",
    "npm": ">= 7.6.3"
  }
}
