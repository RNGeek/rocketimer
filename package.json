{
  "name": "rocketimer",
  "version": "1.0.0",
  "description": "Rocketimer is a cowntdown timer, which is simple and high functional.",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "private": true,
  "repository": "RNGeek/rocketimer",
  "scripts": {
    "start": "yarn run dev",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development",
    "check": "run-p -l -c 'check:tsc --pretty' 'check:eslint --color' check:prettier 'check:jest --colors'",
    "check:tsc": "tsc --noEmit",
    "check:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "check:prettier": "prettier --check .",
    "test": "run-s -l test:*",
    "test:jest": "jest --testPathIgnorePatterns performance",
    "test:perf": "node --expose-gc $(npm bin)/jest --runTestsByPath ./src/performance/App.test.tsx"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "eventemitter3": "^4.0.7",
    "konva": "^7.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.7.0",
    "recoil": "^0.0.10",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "canvas": "^2.6.1",
    "copy-webpack-plugin": "^6.1.1",
    "core-js": "^3.6.5",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "jest-date-mock": "^1.0.8",
    "jest-performance-testing": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "react-performance-testing": "^1.2.3",
